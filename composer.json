{
    "name": "li-bao-jia/we-chat-devtools",
    "description": "Instruct WeChat devtools to perform operations such as login, preview, and upload.",
    "version": "1.0.0",
    "keywords": [
        "li-bao-jia",
        "we-chat-devtools"
    ],
    "homepage": "https://github.com/li-bao-jia/we-chat-devtools",
    "authors": [
        {
            "name": "BaoJia Li",
            "email": "livsyitian@163.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "ixudra/curl": "^6.8"
    },
    "autoload": {
        "psr-4": {
            "BaoJiaLi\\WeChatDevtools\\": "src/"
